using System;
class Program
{
    static void Main()
    {
        int[]arr=new int[10];
        Random rnd=new Random();
        int count=0;

        for(int i=0;i<arr.Length;i++)
        {
            arr[i]=rnd.Next(1,10);

            if(arr[i]%2==0)
            {
                count++;
            }
        }
        Console.WriteLine(count);
    }
}

